{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngRhombusApp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ng-rhombus-app",
      "sourceRoot": "projects/ng-rhombus-app/src",
      "prefix": "ng-rhombus-app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/ng-rhombus-app",
            "index": "projects/ng-rhombus-app/src/index.html",
            "browser": "projects/ng-rhombus-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/ng-rhombus-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ng-rhombus-app/src/favicon.ico",
              "projects/ng-rhombus-app/src/assets"
            ],
            "styles": [
              "projects/ng-rhombus-app/src/theme-colors.scss",
              "projects/ng-rhombus-app/src/custom-theme.scss",
              "projects/ng-rhombus-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
				"fileReplacements": [
                {
                  "replace": "projects/ng-rhombus-app/src/environments/environment.ts",
                  "with": "projects/ng-rhombus-app/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ngRhombusApp:build:production"
            },
            "development": {
              "buildTarget": "ngRhombusApp:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "ngRhombusApp:build"
          }
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/ng-rhombus-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ng-rhombus-app/src/favicon.ico",
              "projects/ng-rhombus-app/src/assets"
            ],
            "styles": [
              "projects/ng-rhombus-app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "@doug-williamson/ng-rhombus": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ng-rhombus",
      "sourceRoot": "projects/ng-rhombus/src",
      "prefix": "ng-rhombus",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/ng-rhombus/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-rhombus/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-rhombus/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ng-rhombus/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "ngRhombusAdmin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ng-rhombus-admin",
      "sourceRoot": "projects/ng-rhombus-admin/src",
      "prefix": "ng-rhombus-admin",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/ng-rhombus-admin",
            "index": "projects/ng-rhombus-admin/src/index.html",
            "browser": "projects/ng-rhombus-admin/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/ng-rhombus-admin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "stylePreprocessorOptions": {
              "sass": {
                "silenceDeprecations": [
                  "import"
                ]
              }
            },
            "assets": [
				"projects/ng-rhombus-admin/src/favicon.ico",
              	"projects/ng-rhombus-admin/src/assets"
            ],
            "styles": [
              "projects/ng-rhombus-admin/src/theme-colors.scss",
              "projects/ng-rhombus-admin/src/custom-theme.scss",
              "projects/ng-rhombus-admin/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/ng-rhombus-admin/src/environments/environment.ts",
                  "with": "projects/ng-rhombus-admin/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ngRhombusAdmin:build:production"
            },
            "development": {
              "buildTarget": "ngRhombusAdmin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/ng-rhombus-admin/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/ng-rhombus-admin/public"
              }
            ],
            "styles": [
              "projects/ng-rhombus-admin/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "ef4c2cb3-2f0b-43b2-ac14-21d6d873193d"
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}