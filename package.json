{
  "name": "ng-rhombus",
  "version": "0.0.6",
  "scripts": {
    "ng": "ng",
    "start:app": "ng serve ngRhombusApp --port 4201",
    "start:admin": "ng serve ngRhombusAdmin --port 4202",
    "build:admin": "ng build ngRhombusAdmin",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:library": "ng build @doug-williamson/ng-rhombus",
    "package:library": "npm pack ./dist/doug-williamson/ng-rhombus",
    "publish:library": "npm publish --access=public ./dist/doug-williamson/ng-rhombus",
    "build:app": "ng build ngRhombusApp",
    "bundle-report": "ng build ngRhombusApp --source-map && source-map-explorer dist/ng-rhombus-app/browser/*.js",
    "emulator": "lsof -ti:8080 | xargs kill -9;  firebase emulators:start --only firestore,auth,storage --import test-data --export-on-exit test-data --project rhombus-78977"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.7",
    "@angular/cdk": "^19.1.5",
    "@angular/common": "^19.1.7",
    "@angular/compiler": "^19.1.7",
    "@angular/core": "^19.1.7",
    "@angular/fire": "^19.0.0",
    "@angular/forms": "^19.1.7",
    "@angular/material": "^19.1.5",
    "@angular/platform-browser": "^19.1.7",
    "@angular/platform-browser-dynamic": "^19.1.7",
    "@angular/router": "^19.1.7",
    "@ngrx/signals": "^19.0.0",
    "@ngrx/store": "^19.0.0",
    "@ngrx/store-devtools": "^19.0.0",
    "@tailwindcss/postcss": "^4.0.6",
    "firebase": "^10.9.0",
    "marked": "^15.0.6",
    "ngx-markdown": "^19.0.0",
    "postcss": "^8.5.2",
    "prismjs": "^1.30.0",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.0.6",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.8",
    "@angular/cli": "^19.1.8",
    "@angular/compiler-cli": "^19.1.7",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.0.1",
    "typescript": "~5.5.4"
  }
}
